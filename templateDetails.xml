<?xml version="1.0" encoding="utf-8" ?>
<!--DOCTYPE install PUBLIC "-//Joomla! 1.7//DTD template 1.0//EN" "http://www.joomla.org/xml/dtd/1.7/template-install.dtd" -->
<extension version="3.3" type="template" client="site">
    <name>Jeprodev</name>
    <version>0.0.0.1</version>
    <creationDate>2/02/2012</creationDate>
    <author>jeprodev</author>
    <authorUrl>jeprodev.net</authorUrl>
    <authorEmail>development@jeprodev.net</authorEmail>

    <updateservers>
        <server type="extension" name="jeprodev" priority="1" >http://www.github.com/jeprodev/jeproshop/jeproshop_update.xml</server>
    </updateservers>

    <files>
        <filename>component.php</filename>
        <filename>error.php</filename>
        <filename>favicon.ico</filename>
        <filename>index.php</filename>
        <filename>templateDetails.xml</filename>
    </files>

    <config>
        <fields name="params" >
            <fieldset name="basic">
                <field name="jeprodev_large_screen_width" type="text" default="1200" label="TPL_JEPRODEV_LARGE_SCREEN_WIDTH_LABEL" description="TPL_JEPRODEV_LARGE_SCREEN_WIDTH_DESC" />
                <field name="jeprodev_medium_screen_width" type="text" default="1000" label="TPL_JEPRODEV_MEDIUM_SCREEN_PAGE_WIDTH_LABEL" description="TPL_JEPRODEV_MEDIUM_SCREEN_WIDTH_DESC" />
            </fieldset>
        </fields>
    </config>
</extension>